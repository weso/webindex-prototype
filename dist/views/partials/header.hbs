<header class="site-header">
  <div class="top">
    <a href="thewebindex.org" class="logo">
      <img src="images/webindex.png" alt="" />
    </a>
    <p class="donate">
      <a href="http://www.webfoundation.org/" target="_blank" title="link opens in new window">Donate now</a>
    </p>
    <nav>
      <ul>
        {{#navigation}}
          {{#if selected}}
            <li class="active">
          {{else}}
            <li>
          {{/if}}
            <a href="/{{path}}">{{label}}</a> <span class="separator">|</span>
          </li>
        {{/navigation}}
      </ul>
    </nav>
  </div>
</header>
